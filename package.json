{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "main": "./dist/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon -w *.*.ts --exec ts-node ./src/main.ts",
    "dev": "cross-env NODE_ENV=test nodemon -w *.*.ts --exec ts-node ./src/main.ts",
    "test": "mocha src/__test__/**/*.test.ts src/api/**/*.test.ts",
    "test_cover": "istanbul cover node_modules/mocha/bin/_mocha src/api/**/*.test.ts",
    "docs": "documentation build api/**/*.*.ts --format md --github -o ./docs/api/image.md",
    "prestart:prod": "tsc"
  },
  "dependencies": {
    "@nestjs/common": "^4.3.0",
    "@nestjs/core": "^4.3.0",
    "@nestjs/microservices": "^4.3.0",
    "@nestjs/testing": "^4.3.0",
    "@nestjs/websockets": "^4.3.0",
    "body-parser": "^1.18.2",
    "class-transformer": "^0.1.8",
    "class-validator": "^0.7.3",
    "connect-multiparty": "^2.1.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "dotenv": "^4.0.0",
    "emailjs": "^1.0.12",
    "excel-stream": "^1.1.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.15.6",
    "http-proxy": "^1.16.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "lz-string": "^1.4.4",
    "moment": "^2.19.3",
    "mongoose": "^5.0.1",
    "node-uuid": "^1.4.8",
    "nodemailer": "^4.6.4",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "request": "^2.83.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.0",
    "socket.io": "^2.0.4",
    "typescript": "^2.5.2",
    "xlsx-populate": "^1.11.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.28",
    "@types/mongoose": "^4.7.23",
    "cross-env": "^5.1.0",
    "nodemon": "^1.12.1",
    "ts-node": "^3.3.0"
  }
}
